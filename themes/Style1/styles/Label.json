{
  ".label": {
    "width": 250,
    "height": 70,
    "touchEnabled": true,
    "text": "Label",
    "visible": true,
    "multiline": false,
    "backgroundColor": "#FFFFFF",
    "alpha": 1,
    "borderColor": "rgba(0,0,0,1)",
    "borderWidth": 0,
    "textColor": "#000000",
    "textAlignment": "MIDLEFT",
    "font": {
      "size": 16,
      "bold": false,
      "italic": false,
      "family": "Lato"
    },
    "flexProps": {
      "alignSelf": "AUTO",
      "positionType": "RELATIVE"
    },
    "&-small": {
      "width": 100
    },
    "&-medium": {
      "width": 250
    },
    "&-large": {
      "width": 350
    },
    "&-warning": {
      "backgroundColor": "#9F6000"
    },
    "&-error": {
      "backgroundColor": "#D8000C"
    },
    "&-success": {
      "backgroundColor": "#4F8A10"
    },
    "&-list-item-header": {
      "textColor": "#1775D0",
      "font": {
          "size": 18
      }
    },
    "&-list-item-title": {
      "textColor": "#4A4A4A",
      "font": {
          "size": 14
      }
    },
    "&-list-item-value": {
      "textColor": "#9B9B9B",
      "font": {
          "size": 14
      }
    }
  }
}
